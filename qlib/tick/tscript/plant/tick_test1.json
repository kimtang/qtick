{
    "global": {
        "admin": {
            "basePort": 30030,
            "host.S": "localhost"
        },
        "qdata": {
            "basePort": 31030,
            "host.S": "localhost"
        }
    },
    "admin": {
        "dataDir.S":"data1",        
        "process": {
            "bus": {
                "lib": "randomSeed,setPort,bus.server,berror.client,heartbeat.client",
                "arg": {
                    "setPort": {
                        "port.q": "%basePort% + 1"
                    }
                }
            },
            "tick": {
                "lib": "randomSeed,setPort,bus.client,berror.server,tick,tick.stream,tick.hft,tick.legacy,heartbeat.server",
                "arg": {
                    "setPort": {
                        "port.q": "%basePort% + 3"
                    }
                }
            },
            "rdb": {
                "lib": "randomSeed,setPort,bus.client,berror.client,tick,tick.sub,heartbeat.client",
                "arg": {
                    "setPort": {
                        "port.q": "%basePort% + 5"
                    }
                }
            },
            "replay": {
                "lib": "randomSeed,setPort,bus.client,berror.client,tick,tick.replay,heartbeat.client",
                "arg": {
                    "setPort": {
                        "port.q": "%basePort% + 6"
                    }
                }
            },
            "cdb": {
                "lib": "randomSeed,setPort,bus.client,berror.client,tick,tick.cdb,heartbeat.client",
                "arg": {
                    "setPort": {
                        "port.q": "%basePort% + 7"
                    }
                }
            }
        },
        "schema": {
            "heartbeat": {
                "column": "date,time,sym,pid,used,heap,peak,wmax,mmap,mphy,syms,symw",
                "tipe": "dpsjjjjjjjjj",
                "rattr": "*sg*********",
                "storage":{"type.S":"partition","mode.S":"auto","partitionCol.S":"date","partAttrCol.S":"sym","sortCol.S":"time"},
                "tick.S": "default",
                "rdb.S": "default",
                "hdb.S": "default",
                "cdb.S":"default",
                "replay.S": "default"
            },
            "beforeExecution": {
                "column": "date,time,sym,etime,guid,user,hdl,host,arg",
                "tipe": "dpspgsi**"
            },
            "afterExecution": {
                "column": "date,time,sym,ftime,guid,error",
                "tipe": "dpspgs"
            },
            "berror": {
                "column": "date,time,sym,action,mode,time,etime,seq,eseq,result,arg,error",
                "tipe": "dpsssppjj**s"
            },
            "reference": {
                "column": "date,time,sym,prx,qty",
                "tipe": "dpsfi",
                "storage.S":"splay"
            },
            "trade": {
                "column": "date,time,sym,prx,qty",
                "tipe": "dpsfi"
            }
        }
    }
}